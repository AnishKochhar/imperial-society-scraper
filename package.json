{
  "name": "imperial-email-scraper",
  "version": "1.0.0",
  "description": "Scraper for Imperial College society emails with AI-powered categorization",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "tsx src/test.ts",
    "test:pipeline": "tsx src/pipeline-test.ts",
    "pipeline": "tsx src/pipeline.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\""
