    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.2",
    "playwright": "^1.40.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
